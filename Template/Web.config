<configuration>
  <configSections>
    <section name="hibernate-configuration" type="NHibernate.Cfg.ConfigurationSectionHandler, NHibernate" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="BundleTable.EnableOptimizations" value="false" />
  </appSettings>
  <system.web>
    <authentication mode="Forms">
      <forms loginUrl="~/login" cookieless="UseCookies" timeout="2880" name=".XERNEASFORMAUTH" path="/" />
    </authentication>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <machineKey decryptionKey="23E78A0374D8AA9FF8374A14F70ABB28C470B2FFF331A8EA" validationKey="4512C0EB7C4F326A3C15BB08978C4A2CAD40B0252E3AA85F108E8D13D546924DED7DC20C2E6ABE13A382EC82E5101E240D1EDEC938D7AE527506D4448F7328B1" />
  </system.web>
  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
    <session-factory name="Aula">
      <property name="connection.driver_class">NHibernate.Driver.SqlClientDriver</property>
      <property name="connection.connection_string">
        Data Source=localhost\developer;Initial Catalog=Template;User Id=sa;Password=123;
      </property>
      <property name="dialect">NHibernate.Dialect.MsSql2008Dialect</property>
      <property name="connection.isolation">ReadCommitted</property>
      <property name="current_session_context_class">NHibernate.Context.WebSessionContext, NHibernate</property>
      <property name="show_sql">true</property>
      <property name="generate_statistics">true</property>
      <!--<property name="hbm2ddl.auto">create-drop</property>-->
      <mapping assembly="Template" />
      <event type="pre-update">
        <!--<listener type="pre-update" class="Thunder.Audit.EventListener, Thunder.Audit" />-->
        <listener type="pre-update" class="Thunder.Data.Pattern.CreatedAndUpdatedPropertyEventListener, Thunder" />
      </event>
      <!--<listener type="post-insert" class="Thunder.Audit.EventListener, Thunder.Audit" />
      <listener type="pre-delete" class="Thunder.Audit.EventListener, Thunder.Audit" />-->
      <listener type="pre-insert" class="Thunder.Data.Pattern.CreatedAndUpdatedPropertyEventListener, Thunder" />
    </session-factory>
  </hibernate-configuration>

  <log4net>
    <appender name="Trace" type="log4net.Appender.TraceAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-5level %logger - %message %newline" />
      </layout>
    </appender>
    <logger name="NHibernate.SQL" additivity="false">
      <level value="DEBUG" />
      <appender-ref ref="Trace" />
    </logger>
    <logger name="NHibernate">
      <level value="WARN" />
      <appender-ref ref="Trace" />
    </logger>
    <logger name="Application">
      <level value="DEBUG" />
      <appender-ref ref="Trace" />
    </logger>
  </log4net>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin.Security.OAuth" publicKeyToken="31bf3856ad364e35" />
